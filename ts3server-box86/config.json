{
  "name": "TS3 Server (Box64)",
  "version": "1.0.0",
  "slug": "ts3server-box64",
  "description": "TeamSpeak 3 Server mit Box64-Emulation und Admin-Interface",
  "startup": "system",
  "boot": "auto",
  "ingress": true,
  "panel_icon": "mdi:account-voice",
  "icon": "icon.svg",
  "ports": {
    "9987/udp": 9987,
    "10011/tcp": 10011,
    "30033/tcp": 30033,
    "5000/tcp": 5000
  },
  "arch": ["aarch64"],
  "map": ["config"],
  "options": {
    "serveradmin_password": "{{ env_var('TS3_ADMIN_PASSWORD') }}"
  },
  "schema": {
    "serveradmin_password": "str"
  }
}
