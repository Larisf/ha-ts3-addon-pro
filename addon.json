{
    "name": "TS3 Server (Box64)",
    "version": "1.0.1",
    "slug": "ts3server-box64",
    "description": "TeamSpeak 3 Server mit Box64-Emulation und modernem GUI.",
    "startup": "system",
    "boot": "auto",
    "ingress": true,  # Wichtig für den Zugriff von außen
    "ports": {
      "9987/udp": 9987,
      "10011/tcp": 10011,
      "30033/tcp": 30033,
      "8080/tcp": 8080 # Port für das Web-GUI
    },
    "arch": ["aarch64"],
    "map": ["config", "backup"], # Backup-Ordner hinzufügen
    "options": {
      "license_key": ""
    },
    "schema": {
      "license_key": "str?"
    },
    "panel_icon": "mdi:teamspeak", # Teamspeak Icon
    "webui": "/app/web/index.html" # Pfad zum GUI
  }